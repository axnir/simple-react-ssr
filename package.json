{
  "name": "simple-react-ssr",
  "version": "1.0.0",
  "description": "a simple react ssr project",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"pnpm client:dev\" \"pnpm server:dev\" \"node dist/server/app.js\"",
    "build": "concurrently \"pnpm client:prod\" \"pnpm server:prod\"",
    "client:prod": "cross-env NODE_ENV=development webpack --config build/webpack-client-config.js",
    "client:dev": "cross-env NODE_ENV=production webpack --config build/webpack-client-config.js",
    "server:prod": "cross-env NODE_ENV=development webpack --config build/webpack-server-config.js",
    "server:dev": "cross-env NODE_ENV=production webpack --config build/webpack-server-config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axnir/simple-react-ssr.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/axnir/simple-react-ssr/issues"
  },
  "homepage": "https://github.com/axnir/simple-react-ssr#readme",
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/register": "^7.17.7",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.14.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "isomorphic-style-loader": "^5.3.2",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "mini-css-extract-plugin": "^2.6.0",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "typescript": "^4.6.4",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "express": "^4.18.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0"
  }
}
